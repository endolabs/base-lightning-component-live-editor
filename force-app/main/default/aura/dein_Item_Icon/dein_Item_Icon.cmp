<aura:component >
    <aura:attribute name="code" type="String" />
    <aura:handler name="init" value="{!this}" action="{!c.update}"/>
    
    <c:dein_Util aura:id="util" />
    
    <h1 class="slds-text-heading--large slds-truncate slds-m-bottom--medium">Icon</h1>
    <c:dein_Alert />	

    <h2 class="slds-text-heading--medium">Preview</h2>
    <c:dein_Preview aura:id="preview" />
    <lightning:icon iconName="action:approval" size="large" alternativeText="Indicates approval"/>
    <lightning:icon iconName="utility:error" variant="error"/>
    
    <h2 class="slds-text-heading--medium">Code</h2>    
    <c:dein_Highlight aura:id="highlight" />

</aura:component>